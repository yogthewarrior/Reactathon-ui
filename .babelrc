{
  "presets": [
  	["env", {"modules": false}],
  	"react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "testing": {
      "presets": ["react", "env"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "server": {
      "presets": ["react", "env"]
    },
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "react-hot-loader/babel"
      ]
    }
  }
}
